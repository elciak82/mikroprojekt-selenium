{
  "name": "@heroku-cli/plugin-config",
  "version": "1.0.0",
  "author": "Jeff Dickey @jdxcode",
  "bugs": "https://github.com/heroku/heroku-cli-plugin-config/issues",
  "dependencies": {
    "@heroku-cli/color": "^1.1.3",
    "@heroku-cli/command": "^8.0.5",
    "@oclif/command": "1",
    "@oclif/config": "1",
    "cli-ux": "^3.3.31",
    "edit-string": "^1.1.5",
    "lodash": "^4.17.5",
    "shell-quote": "^1.6.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1",
    "@oclif/plugin-help": "1",
    "@oclif/test": "1",
    "@oclif/tslint": "1",
    "@types/chai": "4",
    "@types/lodash": "^4.14.107",
    "@types/mocha": "5",
    "@types/node": "9",
    "@types/supports-color": "^5.3.0",
    "chai": "4",
    "globby": "8",
    "mocha": "5",
    "nyc": "11",
    "ts-node": "6",
    "tslib": "1",
    "tslint": "5",
    "typescript": "2.8"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/lib"
  ],
  "homepage": "https://github.com/heroku/heroku-cli-plugin-config",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "heroku",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "heroku/heroku-cli-plugin-config",
  "scripts": {
    "postpack": "rm -f .oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
