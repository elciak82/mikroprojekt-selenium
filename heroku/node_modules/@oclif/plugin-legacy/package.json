{
  "name": "@oclif/plugin-legacy",
  "description": "converts older style plugins to be compatible with oclif",
  "version": "1.0.11",
  "author": "Jeff Dickey @jdxcode",
  "bugs": "https://github.com/oclif/plugin-legacy/issues",
  "dependencies": {
    "@heroku-cli/color": "^1.1.3",
    "@heroku-cli/command": "^8.0.5",
    "@oclif/command": "^1.4.13",
    "ansi-escapes": "^3.1.0",
    "debug": "^3.1.0",
    "semver": "^5.5.0"
  },
  "devDependencies": {
    "@heroku-cli/plugin-apps": "^2.4.37",
    "@oclif/config": "^1.6.5",
    "@oclif/dev-cli": "^1.13.1",
    "@oclif/errors": "^1.0.4",
    "@oclif/plugin-help": "^1.2.4",
    "@oclif/test": "^1.0.5",
    "@oclif/tslint": "^1.1.0",
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "@types/node": "^9.6.6",
    "@types/semver": "^5.5.0",
    "@types/supports-color": "^5.3.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "globby": "^8.0.1",
    "heroku-apps": "^2.4.22",
    "heroku-status": "^3.0.10",
    "mocha": "^5.1.1",
    "nyc": "^11.7.1",
    "ts-node": "^6.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/.oclif.manifest.json",
    "/lib"
  ],
  "homepage": "https://github.com/oclif/plugin-legacy",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "hooks": {
      "init": "./lib/hooks/init"
    },
    "devPlugins": [
      "@oclif/plugin-help",
      "@heroku-cli/plugin-apps",
      "heroku-status"
    ],
    "bin": "oclif-example"
  },
  "repository": "oclif/plugin-legacy",
  "scripts": {
    "postpack": "rm -f .oclif.manifest.json && rm -f .oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
